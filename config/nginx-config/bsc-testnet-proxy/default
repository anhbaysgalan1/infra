server {
        listen 80 default_server;
        listen [::]:80 default_server;

        root /var/www/html;

        index index.html index.htm index.nginx-debian.html;

        server_name _;

        location / {
                if ($http_STRATEGY = 'BAD') {
                        return 200 '{"status": "bad"}';
                }
                proxy_pass https://data-seed-prebsc-1-s1.binance.org:8545;
        }
}